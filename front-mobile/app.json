{
  "expo": {
    "name": "DS - Delivery",
    "slug": "front-mobile",
    "plataforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#DA5C5C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devsuperior.dsdeliver"
    },
    "android": {
      "adaptiveIcon": {
        "package": "com.devsuperior.dsdeliver",
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#DA5C5C"
      }
    }
  }
}
